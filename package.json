{
    "name": "pogo",
    "displayName": "POGO",
    "repository": {
        "url": "https://github.com/evgenyk/pogo_vscode"
    },
    "author": {
        "email": "evgeny@komarevtsev.net",
        "name": "Evgeny Komarevtsev"
    },
    "description": "POGO Stack language support for VS Code",
    "version": "0.0.2",
    "publisher": "evgenyk",
    "engines": {
        "vscode": "^1.28.2"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pogo",
                "aliases": [
                    "pogo"
                ],
                "extensions": [
                    "pogo"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pogo",
                "scopeName": "source.pogo",
                "path": "./syntaxes/pogo.tmLanguage.json"
            }
        ]
    }
}